{
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "config/webpack/base.config.js"
      }
    }
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "rules": {
    "jsx-a11y/href-no-hash": 0,
    "react/jsx-closing-bracket-location": 1,
    "react/jsx-no-bind": 0,
    "react/jsx-curly-spacing": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0
  }
}